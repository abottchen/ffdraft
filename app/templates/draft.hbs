<table class="table table-striped table-bordered table-condensed">
  <thead class="thead-dark">
    <tr class="info">
      <th>Name</th>
      <th>Team</th>
      <th>Pos</th>
      <th>Owner</th>
    </tr>
  </thead>
  <tbody>
    {{#each model as |player|}}
    <tr>
{{!--      <td><img src={{player.picture}} width=600 height=436 /></td> --}}
{{!--      <td><img src={{player.picture}} width=150 height=109 /></td> --}}
      <td>{{player.name}}</td>
      <td>{{player.position}}</td>
      <td>{{player.nflteam}}</td>
      <td>{{player.team}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>
{{outlet}}
